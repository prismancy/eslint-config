{
  "name": "@in5net/eslint-config",
  "type": "module",
  "version": "0.15.0",
  "packageManager": "pnpm@10.0.0",
  "description": "in5net's ESLint config",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "prettier": "@in5net/prettier-config",
  "scripts": {
    "build": "nr typegen && tsup --clean --dts",
    "stub": "tsup",
    "dev": "npx @eslint/config-inspector --config eslint.config.ts",
    "build:inspector": "pnpm build && npx @eslint/config-inspector build",
    "watch": "tsup --watch",
    "lint": "eslint .",
    "typegen": "tsx scripts/typegen.ts",
    "prepack": "nr build",
    "release": "bumpp && pnpm publish",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "eslint": "^9.10.0",
    "eslint-plugin-svelte": ">=2.35.1",
    "svelte-eslint-parser": ">=0.37.0"
  },
  "peerDependenciesMeta": {
    "eslint-plugin-svelte": {
      "optional": true
    },
    "svelte-eslint-parser": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/install-pkg": "^1.0.0",
    "@clack/prompts": "^0.9.1",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.1",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "eslint-config-flat-gitignore": "^2.0.0",
    "eslint-flat-config-utils": "^2.0.1",
    "eslint-merge-processors": "^1.0.0",
    "eslint-plugin-antfu": "^3.0.0",
    "eslint-plugin-command": "^3.0.0",
    "eslint-plugin-import-x": "^4.6.1",
    "eslint-plugin-jsdoc": "^50.6.3",
    "eslint-plugin-jsonc": "^2.19.1",
    "eslint-plugin-n": "^17.15.1",
    "eslint-plugin-perfectionist": "^4.7.0",
    "eslint-plugin-regexp": "^2.7.0",
    "eslint-plugin-unicorn": "^56.0.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15.14.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^1.0.0",
    "parse-gitignore": "^2.0.0"
  },
  "devDependencies": {
    "@antfu/ni": "^23.3.1",
    "@eslint/config-inspector": "^1.0.0",
    "@in5net/eslint-config": "workspace:*",
    "@in5net/prettier-config": "^1.4.2",
    "@types/node": "^22.13.0",
    "bumpp": "^10.0.1",
    "eslint": "^9.19.0",
    "eslint-plugin-svelte": "^2.46.1",
    "eslint-typegen": "^1.0.0",
    "jiti": "^2.4.2",
    "svelte": "^5.19.6",
    "svelte-eslint-parser": "^0.43.0",
    "tsup": "^8.3.6",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "resolutions": {
    "@eslint-community/eslint-utils": "^4.4.1",
    "@typescript-eslint/utils": "^8.22.0",
    "eslint": "^9.19.0",
    "tsx": "^4.19.2"
  }
}
